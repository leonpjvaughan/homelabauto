write-kubeconfig-mode: "0644"
token: {{ hostvars['km1']['token'] }}
server: https://{{ hostvars['km1']['ansible_host'] }}:9345
tls-san:
  - {{ vip }}
  - {{ hostvars['km1']['ansible_host'] }}
  - {{ hostvars['km2']['ansible_host'] }}
  - {{ hostvars['km3']['ansible_host'] }}
node-label:
  - server=true
node-taint:
  - "CriticalAddonsOnly=true:NoExecute"
disable:
  - rke2-ingress-nginx